		function Library.TurnKeybindList(self)
			self.KeybindList.Visible = not self.KeybindList.Visible
			self.KeybindList.Frame.Visible = self.KeybindList.Visible
		end

		function Library.CreateMobileButton(self)
			local mobileButton = Instance.new("TextButton")
			mobileButton.Name = "MobileButton"
			mobileButton.Text = ""
			mobileButton.AnchorPoint = Vector2.new(0.5, 0)
			mobileButton.AutomaticSize = Enum.AutomaticSize.XY
			mobileButton.BackgroundColor3 = Color3.fromRGB(16, 16, 16)
			mobileButton.BackgroundTransparency = 1
			mobileButton.Position = UDim2.new(0.5, 0, 0, 30)
			mobileButton.Size = UDim2.fromOffset(50, 50)
			mobileButton.Parent = self.Holder.Parent

			local image = Instance.new("ImageButton")
			image.Name = "Image"
			image.Image = "rbxassetid://72713944850429"
			image.BackgroundTransparency = 1
			image.Size = UDim2.fromOffset(40, 40)
			image.Position = UDim2.fromScale(0.5, 0.5)
			image.AnchorPoint = Vector2.new(0.5, 0.5)
			image.Parent = mobileButton

			MakeDraggable(image, mobileButton)

			local uICorner = Instance.new("UICorner")
			uICorner.CornerRadius = UDim.new(0, 6)
			uICorner.Parent = image

			local stroke = Instance.new("UIStroke")
			stroke.Name = "_CHILD"
			stroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			stroke.Color = Color3.fromRGB(255, 255, 255)
			stroke.Thickness = 2
			stroke.Parent = image

			local gradient = Instance.new("UIGradient")
			gradient.Color = ColorSequence.new({
				ColorSequenceKeypoint.new(0, Color3.fromRGB(255, 255, 255)),
				ColorSequenceKeypoint.new(1, Color3.fromRGB(147, 106, 147)),
			})
			gradient.Rotation = 180
			gradient.Parent = stroke

			local function startRotating()
				local rotationSpeed = 3
				local direction = 1
				local rotation = 0

				while true do
					rotation = rotation + (rotationSpeed * direction)

					if rotation >= 180 then
						direction = -1
					elseif rotation <= -180 then
						direction = 1
					end

					gradient.Rotation = rotation
					task.wait()
				end
			end

			image.MouseButton1Click:Connect(function()
				Library:SetOpen(not Library.Open)
				if Library.Open then
					task.spawn(startRotating)
					TweenService:Create(image, TweenInfo.new(0.3), {
						ImageTransparency = 0,
					}):Play()
				else
					TweenService:Create(image, TweenInfo.new(0.3), {
						ImageTransparency = 0.4,
					}):Play()
				end
			end)

			if Library.Open then
				task.spawn(startRotating)
			end
			return mobileButton
		end

		function Library.AddKeybind(self, uuid, name, key)
			if not self.KeybindList.Frame then
				self:CreateKeybindList()
			end

			local container = self.KeybindList.Frame.ListContainer
			local entry = Instance.new("TextLabel")
			entry.Size = UDim2.new(1, 0, 0, 20)
			entry.BackgroundTransparency = 1
			entry.Text = name .. ": " .. key
			entry.TextColor3 = Color3.fromRGB(255, 255, 255)
			entry.TextSize = 12
			entry.FontFace = Font.new("rbxassetid://12187365364")
			entry.Parent = container

			self.KeybindList.Keybinds[uuid] = {
				Label = entry,
				Name = name,
				Key = key,
				Active = false,
			}
		end

		function Library.RemoveKeybind(self, name)
			if self.KeybindList.Keybinds[name] then
				self.KeybindList.Keybinds[name].Label:Destroy()
				self.KeybindList.Keybinds[name] = nil
			end
		end

		function Library.UpdateKeybindState(self, uuid, active)
			if self.KeybindList.Keybinds[uuid] then
				self.KeybindList.Keybinds[uuid].Active = active

				TweenService:Create(
					self.KeybindList.Keybinds[uuid].Label,
					TweenInfo.new(0.2),
					{ TextColor3 = active and Color3.fromRGB(255, 188, 254) or Color3.fromRGB(255, 255, 255) }
				):Play()
			end
		end
		function Library.ToggleKeybindList(self)
			if not self.KeybindList.Frame then
				self:CreateKeybindList()
			end

			self.KeybindList.Visible = not self.KeybindList.Visible
			self.KeybindList.Frame.Visible = self.KeybindList.Visible
		end

		function Library.UpdateKeybind(self, uuid, key)
			if self.KeybindList.Keybinds[uuid] then
				local keybind = self.KeybindList.Keybinds[uuid]
				keybind.Key = key
				keybind.Label.Text = keybind.Name .. ": " .. key
			end
		end

