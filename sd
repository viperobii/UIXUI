			Var_unknown2.Name = "_"
			Var_unknown2.Padding = UDim.new(0, 12)

			Var_unknown2:GetPropertyChangedSignal("AbsoluteContentSize"):Connect(function()
				Right.Size = UDim2.new(0.5, -6, 0, Var_unknown.AbsoluteContentSize.Y)
			end)

			Var_unknown2.FillDirection = Enum.FillDirection.Vertical
			Var_unknown2.SortOrder = Enum.SortOrder.LayoutOrder
			Var_unknown2.Parent = Right
			Var_unknown2.HorizontalFlex = 1

			Right.Parent = Content

			function Tab.Turn(self, bool)
				Tab.Open = bool

				local targetTransparency = Library.CurrentLayout == "Kavo" and 0.91 or 0.91
				local closedTransparency = Library.CurrentLayout == "Kavo" and 0.97 or 0.93

				TweenService:Create(atab, TweenInfo.new(0.30, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
					BackgroundTransparency = Tab.Open and targetTransparency or closedTransparency,
				}):Play()

				TweenService
					:Create(
						globalSettingsUIScale,
						TweenInfo.new(0.20, Enum.EasingStyle.Sine, Enum.EasingDirection.Out),
						{
							Scale = Tab.Open and 1 or 0,
						}
					)
					:Play()

				if Tab.Open then
					Tabs.Position = UDim2.new(1, 10, 0, 1)
					Tabs.Visible = true

					TweenService:Create(
						Tabs,
						TweenInfo.new(Library.TabAnimSpeed, Enum.EasingStyle.Sine, Enum.EasingDirection.Out),
						{
							Position = UDim2.new(0, 10, 0, 1),
						}
					):Play()
				else
					local exitTween = TweenService:Create(
						Tabs,
						TweenInfo.new(Library.TabAnimSpeed, Enum.EasingStyle.Sine, Enum.EasingDirection.Out),
						{
							Position = UDim2.new(-1, 10, 0, 1),
						}
					)

					exitTween.Completed:Connect(function()
						if not Tab.Open then
							Tabs.Visible = false
						end
					end)

					exitTween:Play()
				end

				Library:Connection(atab.MouseButton1Down, function()
					if not Tab.Open then
						for _, Tabs in pairs(Tab.Window.Tabs) do
							if Tabs.Open and Tabs ~= Tab then
								Tabs:Turn(false)
							end
						end
						Tab:Turn(true)
					end
				end)
			end

			Tab.Elements = {
				Left = Left,
				Right = Right,
			}

			if #Tab.Window.Tabs == 0 then
				Tab:Turn(true)
			end
			Tab.Window.Tabs[#Tab.Window.Tabs + 1] = Tab
			self.Tabs[#self.Tabs + 1] = Tab

			Tab.Window:UpdateTabs()
			return setmetatable(Tab, Library.Tabs)
		end

		function Tabs.Section(self, Properties)
			if not Properties then
				Properties = {}
			end

			local Section = {
				Name = Properties.Name or "Section",
