		function Window.UpdateTabs(self)
			for Index, Tab in pairs(Window.Tabs) do
				Tab:Turn(Tab.Open)
			end
		end

		Library.Holder = SecondDcsgo

		function Library.Tab(self, Properties)
			if not Properties then
				Properties = {}
			end

			local Tab = {
				Name = Properties.Name or Properties.name or "aimbot",
				Window = Window,
				Open = false,
				Sections = {},
				Tabs = {},
				Elements = {},
			}

			local atab = Instance.new("TextButton", Frame14)
			atab.Name = Tab.Name
			atab.Text = ""
			atab.Active = false
			atab.BackgroundColor3 = Color3.fromRGB(72, 72, 72)
			atab.BackgroundTransparency = Library.CurrentLayout == "Kavo" and 0.97 or 0.91
			atab.BorderSizePixel = 0
			atab.Selectable = false
			atab.Size = UDim2.new(0, 82, 1, 0)

			local tabLabel = Instance.new("TextLabel")
			tabLabel.Name = "TextLabel"
			tabLabel.FontFace = Font.new("rbxasset://fonts/families/GothamSSm.json")
			tabLabel.RichText = true
			tabLabel.Text = Tab.Name
			tabLabel.TextColor3 = Color3.fromRGB(240, 240, 240)
			tabLabel.TextSize = 12
			tabLabel.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			tabLabel.BackgroundTransparency = 1
			tabLabel.BorderColor3 = Color3.fromRGB(0, 0, 0)
			tabLabel.Size = UDim2.fromScale(1, 1)

			local tabPadding = Instance.new("UIPadding")
			tabPadding.Name = "UIPadding"
			tabPadding.Parent = tabLabel

			tabLabel.Parent = atab

			local UICorner6 = Instance.new("UICorner")
			UICorner6.Name = "UICorner"
			UICorner6.CornerRadius = UDim.new(0, 6)
			UICorner6.Parent = atab

			local Frame5 = Instance.new("Frame")
			Frame5.Name = "TabIndicator"
			Frame5.BackgroundColor3 = Library.Accent
			Frame5.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Frame5.BorderSizePixel = 0
			Frame5.Position = UDim2.new(0.22, 5, 1.07, -4)
			Frame5.Size = UDim2.fromOffset(35, 4)

			local globalSettingsUIScale = Instance.new("UIScale", Frame5)
			globalSettingsUIScale.Name = "GlobalSettingsUIScale"
			globalSettingsUIScale.Scale = 1e-07

			local UICorner7 = Instance.new("UICorner")
			UICorner7.Name = "UICorner"
			UICorner7.CornerRadius = UDim.new(0, 634)
			UICorner7.Parent = Frame5

			Frame5.Parent = atab

			local Tabs = Instance.new("ScrollingFrame", TabBackground)
			Tabs.Name = "tabs"
			Tabs.ScrollBarThickness = 0
			Tabs.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			Tabs.BackgroundTransparency = 1
			Tabs.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Tabs.BorderSizePixel = 0
			Tabs.ClipsDescendants = false
			Tabs.Position = UDim2.fromOffset(10, 1)
			Tabs.Selectable = false
			Tabs.Size = UDim2.new(1, -20, 1.02, -10)

			local UIPadding = Instance.new("UIPadding")
			UIPadding.Name = "UIPadding"
			UIPadding.PaddingTop = UDim.new(0, 1)
			UIPadding.Parent = Tabs

			local Var_unknown = Instance.new("UIListLayout")
			Var_unknown.Name = "_"
			Var_unknown.Padding = UDim.new(0, 16)
			Var_unknown.FillDirection = Enum.FillDirection.Horizontal
			Var_unknown.SortOrder = Enum.SortOrder.LayoutOrder
			Var_unknown.HorizontalFlex = 1
			Var_unknown.Parent = Tabs

