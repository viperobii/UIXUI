				SectionContent = section2,
			}
			Section.Tab.Sections[#Section.Tab.Sections + 1] = Section
			return setmetatable(Section, Library.Sections)
		end

		function Sections.Toggle(self, Properties)
			if not Properties then
				Properties = {}
			end

			local Toggle = {
				Window = self.Window,
				Tab = self.Tab,
				Section = self,
				Name = Properties.Name or "Toggle",
				State = (
					Properties.state
						or Properties.State
						or Properties.def
						or Properties.Def
						or Properties.default
						or Properties.Default
						or false
				),
				Callback = (
					Properties.callback
						or Properties.Callback
						or Properties.callBack
						or Properties.CallBack
						or function() end
				),
				Flag = (
					Properties.flag
						or Properties.Flag
						or Properties.pointer
						or Properties.Pointer
						or Library.NextFlag()
				),
				Toggled = false,
			}

			local ToggleSwitchActive = Instance.new("TextButton", Toggle.Section.Elements.SectionContent)
			ToggleSwitchActive.Name = "ToggleSwitchActive"
			ToggleSwitchActive.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
			ToggleSwitchActive.Text = ""
			ToggleSwitchActive.TextColor3 = Color3.fromRGB(0, 0, 0)
			ToggleSwitchActive.TextSize = 14
			ToggleSwitchActive.AutoButtonColor = false
			ToggleSwitchActive.AutomaticSize = Enum.AutomaticSize.Y
			ToggleSwitchActive.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ToggleSwitchActive.BackgroundTransparency = 1
			ToggleSwitchActive.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ToggleSwitchActive.BorderSizePixel = 0
			ToggleSwitchActive.Position = UDim2.fromScale(0.00518, 0.0577)
			ToggleSwitchActive.Size = UDim2.new(1, 0, 0, 17)

			local TextLabel9 = Instance.new("TextLabel")
			TextLabel9.Name = "TextLabel"
			TextLabel9.FontFace = Font.new("rbxassetid://12187365364")
			TextLabel9.Text = Toggle.Name
			TextLabel9.TextColor3 = Color3.fromRGB(62, 62, 62)
			TextLabel9.TextSize = 12
			TextLabel9.TextWrapped = true
			TextLabel9.TextXAlignment = Enum.TextXAlignment.Left
			TextLabel9.AnchorPoint = Vector2.new(0, 0.5)
			TextLabel9.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			TextLabel9.BackgroundTransparency = 1
			TextLabel9.BorderColor3 = Color3.fromRGB(0, 0, 0)
			TextLabel9.BorderSizePixel = 0
			TextLabel9.Position = UDim2.new(0, 11, 0.5, -1)
			TextLabel9.Size = UDim2.new(1, -52, 0, 17)
			TextLabel9.Parent = ToggleSwitchActive

			local Frame15 = Instance.new("Frame")
			Frame15.Name = "Frame"
			Frame15.AnchorPoint = Vector2.new(1, 0.5)
			Frame15.BackgroundColor3 = Color3.fromRGB(141, 102, 134)
			Frame15.BackgroundTransparency = 1
			Frame15.BorderColor3 = Color3.fromRGB(0, 0, 0)
			Frame15.BorderSizePixel = 0
			Frame15.Position = UDim2.new(0.985, -10, 0.433, 0)
			Frame15.Size = UDim2.fromOffset(30, 16)

			local UIStroke2 = Instance.new("UIStroke")
			UIStroke2.Name = "UIStroke"
			UIStroke2.Transparency = 0
			UIStroke2.Color = Color3.fromRGB(27, 27, 27)
			UIStroke2.Parent = Frame15

			local UICorner16 = Instance.new("UICorner")
			UICorner16.Name = "UICorner"
			UICorner16.CornerRadius = UDim.new(0, 764)
			UICorner16.Parent = Frame15

			local ImageLabel2 = Instance.new("Frame")
			ImageLabel2.Name = "ImageLabel"
			ImageLabel2.AnchorPoint = Vector2.new(0, 0.5)
			ImageLabel2.BackgroundColor3 = Color3.fromRGB(62, 62, 62)
			ImageLabel2.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ImageLabel2.BorderSizePixel = 0
			ImageLabel2.Position = UDim2.new(0.06, 2, 0.5, 0)
			ImageLabel2.Size = UDim2.fromOffset(10, 10)

			local UICorner17 = Instance.new("UICorner")
			UICorner17.Name = "UICorner"
			UICorner17.CornerRadius = UDim.new(0, 64)
			UICorner17.Parent = ImageLabel2
			ImageLabel2.Parent = Frame15
			Frame15.Parent = ToggleSwitchActive

			local function SetState()
				Toggle.Toggled = not Toggle.Toggled
				print("Toggle State Changed: " .. tostring(Toggle.Toggled))

				TweenService
					:Create(ImageLabel2, TweenInfo.new(0.20, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
						BackgroundColor3 = Toggle.Toggled and Library.Accent or Color3.fromRGB(62, 62, 62),
						Position = Toggle.Toggled and UDim2.new(0.47, 2, 0.5, 0) or UDim2.new(0.06, 2, 0.5, 0),
					})
					:Play()

				TweenService:Create(Frame15, TweenInfo.new(0.20, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
					BackgroundColor3 = Color3.fromRGB(141, 102, 134),
					BackgroundTransparency = Toggle.Toggled and 0.4 or 1,
				}):Play()

				TweenService:Create(UIStroke2, TweenInfo.new(0.20, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
					Transparency = Toggle.Toggled and 1 or 0,
				}):Play()

				TweenService:Create(TextLabel9, TweenInfo.new(0.20, Enum.EasingStyle.Sine, Enum.EasingDirection.Out), {
					TextColor3 = Toggle.Toggled and Color3.fromRGB(255, 255, 255) or Color3.fromRGB(62, 62, 62),
				}):Play()

				Library.Flags[Toggle.Flag] = Toggle.Toggled
				Toggle.Callback(Toggle.Toggled)
			end
			Library:Connection(ToggleSwitchActive.InputBegan, function(input)
				if
					input.UserInputType == Enum.UserInputType.MouseButton1
					or input.UserInputType == Enum.UserInputType.Touch
				then
					SetState()
				end
			end)

			function Toggle.Set(bool)
				bool = type(bool) == "boolean" and bool or false
				if Toggle.Toggled ~= bool then
					SetState()
				end
			end
			Toggle.Set(Toggle.State)
			print(Toggle.State)
			Library.Flags[Toggle.Flag] = Toggle.State
			Flags[Toggle.Flag] = Toggle.Set

			Toggle.Set(Toggle.State)
			Library.Flags[Toggle.Flag] = Toggle.State
			Flags[Toggle.Flag] = Toggle.Set
			return Toggle
		end
		function Sections.Textbox(self, Properties)
			local Textbox = {
				Name = Properties.Name or "Textbox",
				Default = Properties.Default or "",
				PlaceholderText = Properties.PlaceholderText or "Placeholder",
				Flag = Properties.Flag or Library.NextFlag(),
				Callback = Properties.Callback or function() end,
				Section = self,
			}

			local TextboxFrame = Instance.new("TextButton", Textbox.Section.Elements.SectionContent)
			TextboxFrame.Name = "Textbox"
			TextboxFrame.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
			TextboxFrame.Text = ""
			TextboxFrame.TextColor3 = Color3.fromRGB(0, 0, 0)
			TextboxFrame.TextSize = 14
			TextboxFrame.AutoButtonColor = false
			TextboxFrame.AutomaticSize = Enum.AutomaticSize.Y
			TextboxFrame.BackgroundTransparency = 1
			TextboxFrame.Size = UDim2.new(1, 0, 0, 25)

			local TextboxName = Instance.new("TextLabel", TextboxFrame)
			TextboxName.Name = "TextLabel"
			TextboxName.FontFace = Font.new("rbxassetid://12187365364")
			TextboxName.Text = Textbox.Name
			TextboxName.TextColor3 = Color3.fromRGB(62, 62, 62)
			TextboxName.TextSize = 12
			TextboxName.TextWrapped = true
			TextboxName.TextXAlignment = Enum.TextXAlignment.Left
			TextboxName.BackgroundTransparency = 1
			TextboxName.Position = UDim2.new(0, 11, 0.5, 0)
			TextboxName.Size = UDim2.new(1, -102, 0, 17)
			TextboxName.AnchorPoint = Vector2.new(0, 0.5)

			local TextboxHolder = Instance.new("Frame", TextboxFrame)
			TextboxHolder.Name = "TextboxHolder"
			TextboxHolder.AnchorPoint = Vector2.new(1, 0.5)
			TextboxHolder.BackgroundColor3 = Color3.fromRGB(86, 86, 86)
			TextboxHolder.BackgroundTransparency = 0.95
			TextboxHolder.BorderSizePixel = 0
			TextboxHolder.Position = UDim2.new(1, -15, 0.5, 0)

			local layout = Library.Layouts[Library.CurrentLayout]
			TextboxHolder.Size = layout.Holders.Textbox

			local UICorner = Instance.new("UICorner", TextboxHolder)
			UICorner.CornerRadius = UDim.new(0, 4)

			local UIStroke = Instance.new("UIStroke", TextboxHolder)
			UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			UIStroke.Color = Color3.fromRGB(56, 56, 56)
			UIStroke.Transparency = 0.5

			local TextboxUnderLine = Instance.new("Frame", TextboxHolder)
			TextboxUnderLine.Name = "Frame"
			TextboxUnderLine.AnchorPoint = Vector2.new(0, 1)
			TextboxUnderLine.BackgroundColor3 = Color3.fromRGB(255, 188, 254)
			TextboxUnderLine.BackgroundTransparency = 1
			TextboxUnderLine.BorderSizePixel = 0
			TextboxUnderLine.Position = UDim2.new(0, 2, 1, 0)
			TextboxUnderLine.Size = UDim2.new(1, -4, 0, 1)

			local Textboxframe = Instance.new("Frame", TextboxHolder)
			Textboxframe.Name = "Frame"
			Textboxframe.BackgroundTransparency = 1
			Textboxframe.BorderSizePixel = 0
			Textboxframe.ClipsDescendants = true
			Textboxframe.Position = UDim2.fromOffset(6, 0)
			Textboxframe.Size = UDim2.new(1, -12, 1, 0)

			local TextBox = Instance.new("TextBox", Textboxframe)
			TextBox.Name = "TextBox"
			TextBox.ClearTextOnFocus = false
			TextBox.FontFace = Font.new("rbxassetid://12187365364")
			TextBox.PlaceholderColor3 = Color3.fromRGB(170, 170, 170)
			TextBox.PlaceholderText = Textbox.Default
			TextBox.Text = ""
			TextBox.TextColor3 = Color3.fromRGB(240, 240, 240)
			TextBox.TextSize = 12
			TextBox.TextXAlignment = Enum.TextXAlignment.Left
			TextBox.AutomaticSize = Enum.AutomaticSize.Y
			TextBox.BackgroundTransparency = 1
			TextBox.Position = UDim2.fromOffset(2, 0)
			TextBox.Size = UDim2.fromScale(1, 1)

			Library:Connection(TextBox.Focused, function()
				TweenService:Create(TextboxUnderLine, TweenInfo.new(0.2), {
					BackgroundTransparency = 0.5,
				}):Play()
				TweenService:Create(TextboxName, TweenInfo.new(0.2), {
					TextColor3 = Color3.fromRGB(255, 255, 255),
				}):Play()
				TweenService:Create(TextboxHolder, TweenInfo.new(0.2), {
					BackgroundTransparency = 0.87,
				}):Play()
			end)

			Library:Connection(TextBox.FocusLost, function(enterPressed)
				TweenService:Create(TextboxUnderLine, TweenInfo.new(0.2), {
					BackgroundTransparency = 1,
				}):Play()

				TweenService:Create(TextboxName, TweenInfo.new(0.2), {
					TextColor3 = Color3.fromRGB(62, 62, 62),
				}):Play()

				TweenService:Create(TextboxHolder, TweenInfo.new(0.2), {
					BackgroundTransparency = 0.95,
				}):Play()

				if enterPressed then
					Library.Flags[Textbox.Flag] = TextBox.Text
					Textbox.Callback(TextBox.Text)
				end
			end)

			Library:Connection(TextBox:GetPropertyChangedSignal("Text"), function()
				if TextBox:IsFocused() then
					TextboxUnderLine.BackgroundTransparency = 0.5
				end
			end)

			function Textbox.Set(self, Text)
				TextBox.Text = Text
				Library.Flags[Textbox.Flag] = Text
				Textbox.Callback(Text)
			end

			Library.Flags[Textbox.Flag] = Textbox.Default

			return Textbox
		end

		function Sections.Keybind(self, Properties)
			local UUID = game:GetService("HttpService"):GenerateGUID(false)

			local Properties = Properties or {}
			local Keybind = {
				UUID = UUID,
				Section = self,
				Name = Properties.name or Properties.Name or "Keybind",
				State = Properties.Key or Properties.key or Enum.KeyCode.E,
				Mode = Properties.Mode or Properties.mode or "Toggle",
				UseKey = Properties.UseKey or false,
				Callback = Properties.Callback or Properties.callback or function() end,
				Flag = Properties.Flag or Properties.flag or Library.NextFlag(),
				Binding = nil,
			}
			local Key
			local State = false
			local c = Library:Connection(game:GetService("RunService").RenderStepped)

			local KeybindFrame = Instance.new("TextButton", Keybind.Section.Elements.SectionContent)
			KeybindFrame.Name = "Keybind"
			KeybindFrame.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
			KeybindFrame.Text = ""
			KeybindFrame.TextColor3 = Color3.fromRGB(0, 0, 0)
			KeybindFrame.TextSize = 14
			KeybindFrame.AutoButtonColor = false
			KeybindFrame.AutomaticSize = Enum.AutomaticSize.Y
			KeybindFrame.BackgroundTransparency = 1
			KeybindFrame.Size = UDim2.new(1, 0, 0, 25)

			local KeybindName = Instance.new("TextLabel", KeybindFrame)
			KeybindName.Name = "TextLabel"
			KeybindName.FontFace = Font.new("rbxassetid://12187365364")
			KeybindName.Text = Keybind.Name
			KeybindName.TextColor3 = Color3.fromRGB(62, 62, 62)
			KeybindName.TextSize = 12
			KeybindName.TextWrapped = true
			KeybindName.TextXAlignment = Enum.TextXAlignment.Left
			KeybindName.BackgroundTransparency = 1
			KeybindName.Position = UDim2.new(0, 11, 0.5, 0)
			KeybindName.Size = UDim2.new(1, -102, 0, 17)
			KeybindName.AnchorPoint = Vector2.new(0, 0.5)

			local KeybindHolder = Instance.new("Frame", KeybindFrame)
			KeybindHolder.Name = "KeybindHolder"
			KeybindHolder.AnchorPoint = Vector2.new(1, 0.5)
			KeybindHolder.BackgroundColor3 = Color3.fromRGB(86, 86, 86)
			KeybindHolder.BackgroundTransparency = 0.95
			KeybindHolder.BorderSizePixel = 0
			KeybindHolder.Position = UDim2.new(1, -15, 0.5, 0)

			local layout = Library.Layouts[Library.CurrentLayout]
			KeybindHolder.Size = layout.Holders.Keybind

			local UICorner = Instance.new("UICorner", KeybindHolder)
			UICorner.CornerRadius = UDim.new(0, 4)

			local UIStroke = Instance.new("UIStroke", KeybindHolder)
			UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
			UIStroke.Color = Color3.fromRGB(56, 56, 56)
			UIStroke.Transparency = 0.5

			local KeybindUnderLine = Instance.new("Frame", KeybindHolder)
			KeybindUnderLine.Name = "Frame"
			KeybindUnderLine.AnchorPoint = Vector2.new(0, 1)
			KeybindUnderLine.BackgroundColor3 = Color3.fromRGB(255, 188, 254)
			KeybindUnderLine.BackgroundTransparency = 1
			KeybindUnderLine.BorderSizePixel = 0
			KeybindUnderLine.Position = UDim2.new(0, 2, 1, 0)
			KeybindUnderLine.Size = UDim2.new(1, -4, 0, 1)

			local KeybindText = Instance.new("TextLabel", KeybindHolder)
			KeybindText.Name = "TextLabel"
			KeybindText.FontFace = Font.new("rbxassetid://12187365364")
			KeybindText.Text = (Keybind.State and typeof(Keybind.State) == "EnumItem") and Keybind.State.Name or "..."
			KeybindText.TextColor3 = Color3.fromRGB(240, 240, 240)
			KeybindText.TextSize = 12
			KeybindText.TextXAlignment = Enum.TextXAlignment.Center
			KeybindText.BackgroundTransparency = 1
			KeybindText.Size = UDim2.fromScale(1, 1)
			KeybindText:GetPropertyChangedSignal("Text"):Connect(function()
				KeybindHolder.Size = UDim2.new(0, KeybindText.TextBounds.X + 20, 0, 23)
			end)

			if not Library.KeybindList.Frame then
				Library:CreateKeybindList()
			end

			local function set(newkey)
				if typeof(newkey) == "EnumItem" then
					if c then
						c:Disconnect()
						if Keybind.Flag then
							Library.Flags[Keybind.Flag] = false
						end
						Keybind.Callback(false)
					end

					if tostring(newkey):find("MouseButton") then
						return
					end

					if newkey == Enum.KeyCode.Backspace then
						Key = nil
						KeybindText.Text = "None"
					else
						Key = newkey
						KeybindText.Text = (Library.Keys[newkey] or tostring(newkey):gsub("Enum.KeyCode.", ""))
					end

					if newkey ~= nil then
						Library:UpdateKeybind(
							Keybind.UUID,
							Library.Keys[newkey] or tostring(newkey):gsub("Enum.KeyCode.", "")
						)
					else
						Library:UpdateKeybind(Keybind.UUID, "None")
					end

					Library.Flags[Keybind.Flag .. "_KEY"] = newkey
				end
			end

			if not Library.KeybindList.Frame then
				Library:CreateKeybindList()
			end

			Library:AddKeybind(
				Keybind.UUID,
				Keybind.Name,
				Library.Keys[Keybind.State] or tostring(Keybind.State):gsub("Enum.KeyCode.", "")
			)

			set(Keybind.State)

			KeybindFrame.InputBegan:Connect(function(input)
				if input.UserInputType == Enum.UserInputType.MouseButton1 and not Keybind.Binding then
					Keybind.Binding = true
					KeybindText.Text = "..."

					KeybindUnderLine.BackgroundTransparency = 0.5
					TweenService:Create(KeybindName, TweenInfo.new(0.2), {
						TextColor3 = Color3.fromRGB(255, 255, 255),
					}):Play()
					TweenService:Create(KeybindHolder, TweenInfo.new(0.2), {
						BackgroundTransparency = 0.87,
					}):Play()

					local connection
					connection = game:GetService("UserInputService").InputBegan:Connect(function(Input)
						if Input.UserInputType == Enum.UserInputType.Keyboard then
							set(Input.KeyCode)

							TweenService:Create(KeybindUnderLine, TweenInfo.new(0.2), {
								BackgroundTransparency = 1,
							}):Play()
							TweenService:Create(KeybindName, TweenInfo.new(0.2), {
								TextColor3 = Color3.fromRGB(62, 62, 62),
							}):Play()
							TweenService:Create(KeybindHolder, TweenInfo.new(0.2), {
								BackgroundTransparency = 0.95,
							}):Play()

							connection:Disconnect()
							Keybind.Binding = false
						end
					end)
				end
			end)

			game:GetService("UserInputService").InputBegan:Connect(function(input)
				if
					input.UserInputType == Enum.UserInputType.Keyboard
					and input.KeyCode == Key
					and not Keybind.Binding
				then
					if Keybind.Mode == "Toggle" then
						State = not State
						if Keybind.Flag then
							Library.Flags[Keybind.Flag] = State
						end
						Library:UpdateKeybindState(Keybind.UUID, State)
						Keybind.Callback(State)
					elseif Keybind.Mode == "Hold" then
						State = true
						if Keybind.Flag then
							Library.Flags[Keybind.Flag] = true
						end
						Library:UpdateKeybindState(Keybind.UUID, true)
						c = game:GetService("RunService").RenderStepped:Connect(function()
							Keybind.Callback(true)
						end)
					end
				end
			end)

			game:GetService("UserInputService").InputEnded:Connect(function(input)
				if
					input.UserInputType == Enum.UserInputType.Keyboard
					and input.KeyCode == Key
					and Keybind.Mode == "Hold"
				then
					State = false
					if c then
						c:Disconnect()
						if Keybind.Flag then
							Library.Flags[Keybind.Flag] = false
						end
						Library:UpdateKeybindState(Keybind.UUID, false)
						Keybind.Callback(false)
					end
				end
			end)

			function Keybind.Set(self, key)
				set(key)
			end

			function Keybind.ChangeMode(self, newMode)
				if newMode ~= "Toggle" and newMode ~= "Hold" then
					warn("Must be 'Toggle' or 'Hold'")
					return
				end

				if self.Mode == "Hold" and c then
					c:Disconnect()
					if self.Flag then
						Library.Flags[self.Flag] = false
					end
					Library:UpdateKeybindState(self.UUID, false)
					self.Callback(false)
				end

				self.Mode = newMode
				State = false

				if self.Flag then
					Library.Flags[self.Flag] = false
				end
				Library:UpdateKeybindState(self.UUID, false)
			end

			return Keybind
		end
		
		-- Dropdown code ehre
		function Sections.Dropdown(self, Properties)
    self.Dropdowncount = (self.Dropdowncount or 0) + 1

    local Dropdown = {
        Name = Properties.Name or "Dropdown",
        Options = Properties.Options or {},
        Default = Properties.Default or "",
        Max = Properties.Max,
        ScrollMax = Properties.ScrollMax,
        Flag = Properties.Flag or Library.NextFlag(),
        Callback = Properties.Callback or function() end,
        Zindex = Properties.Zindex or (1000 - self.Dropdowncount),
        Section = self,
        OptionInsts = {},
    }

    local DropdownFrame = Instance.new("TextButton", Dropdown.Section.Elements.SectionContent)
    DropdownFrame.Name = "Dropdown"
    DropdownFrame.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
    DropdownFrame.Text = ""
    DropdownFrame.TextColor3 = Color3.fromRGB(0, 0, 0)
    DropdownFrame.TextSize = 14
    DropdownFrame.ZIndex = Dropdown.Zindex
    DropdownFrame.AutoButtonColor = false
    DropdownFrame.BackgroundTransparency = 1
    DropdownFrame.Size = UDim2.new(1, 0, 0, 25)

    local DropdownName = Instance.new("TextLabel", DropdownFrame)
    DropdownName.Name = "TextLabel"
    DropdownName.FontFace = Font.new("rbxassetid://12187365364")
    DropdownName.Text = Dropdown.Name
    DropdownName.TextColor3 = Color3.fromRGB(62, 62, 62)
    DropdownName.TextSize = 12
    DropdownName.TextWrapped = true
    DropdownName.TextXAlignment = Enum.TextXAlignment.Left
    DropdownName.BackgroundTransparency = 1
    DropdownName.Position = UDim2.new(0, 11, 0.5, 0)
    DropdownName.Size = UDim2.new(1, -133, 0, 17)
    DropdownName.AnchorPoint = Vector2.new(0, 0.5)

    local DropdownHolder = Instance.new("Frame", DropdownFrame)
    DropdownHolder.Name = "DropdownHolder"
    DropdownHolder.AnchorPoint = Vector2.new(1, 0.5)
    DropdownHolder.BackgroundColor3 = Color3.fromRGB(86, 86, 86)
    DropdownHolder.BackgroundTransparency = 0.95
    DropdownHolder.BorderSizePixel = 0
    DropdownHolder.ZIndex = Dropdown.Zindex
    DropdownHolder.Position = UDim2.new(1, -15, 0.5, 0)
    local layout = Library.Layouts[Library.CurrentLayout]

    -- Replace the mobile scaling variables at the top of the function with more moderate values
    local UserInputService = game:GetService("UserInputService")
    local isMobile = UserInputService.TouchEnabled
    
    -- Reduce mobile sizes by another 15%
    -- Change these lines near the top of the function:
    local mobileScale = isMobile and 1.02 or 1 -- Reduced from 1.2 to 1.02
    local optionHeight = isMobile and 26 or 22 -- Reduced from 30 to 26
    local textSize = isMobile and 12 or 11 -- Reduced from 14 to 12
    local dropdownListHeight = isMobile and 136 or 105 -- Reduced from 160 to 136
    local dropdownFrameHeight = isMobile and 26 or 25 -- Reduced from 30 to 26

    -- Modify the DropdownHolder size to be smaller
    if isMobile then
        -- For mobile, make it take up less width
        DropdownHolder.Size = UDim2.new(0.425, 0, 0, 26) -- Reduced from 0.5 to 0.425 width, 30 to 26 height
    else
        DropdownHolder.Size = layout.Holders.Dropdown
    end

    local UICorner = Instance.new("UICorner", DropdownHolder)
    UICorner.CornerRadius = UDim.new(0, 4)

    local UIStroke = Instance.new("UIStroke", DropdownHolder)
    UIStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    UIStroke.Color = Color3.fromRGB(56, 56, 56)
    UIStroke.Transparency = 0.5

    local DropdownText = Instance.new("TextLabel", DropdownHolder)
    DropdownText.Name = "TextLabel"
    DropdownText.FontFace = Font.new("rbxassetid://12187365364")
    DropdownText.Text = ""
    DropdownText.TextColor3 = Library.Accent
    DropdownText.TextSize = 12
    DropdownText.TextXAlignment = Enum.TextXAlignment.Left
    DropdownText.BackgroundTransparency = 1
    DropdownText.Position = UDim2.fromOffset(8, 0)
    DropdownText.Size = UDim2.fromScale(1, 1)
    DropdownText.TextTruncate = Enum.TextTruncate.AtEnd
    DropdownText.Size = UDim2.new(1, -25, 1, 0)

    local DropdownArrow = Instance.new("ImageLabel", DropdownHolder)
    DropdownArrow.Name = "Arrow"
    DropdownArrow.Image = "rbxassetid://10709790948"
    DropdownArrow.ImageColor3 = Color3.fromRGB(170, 170, 170)
    DropdownArrow.AnchorPoint = Vector2.new(1, 0.5)
    DropdownArrow.BackgroundTransparency = 1
    DropdownArrow.Position = UDim2.new(1, -7, 0.5, 0)
    DropdownArrow.Size = UDim2.fromOffset(14, 13)

    local DropdownList = Instance.new("Frame", DropdownHolder)
    DropdownList.Name = "List"
    DropdownList.BackgroundColor3 = Color3.fromRGB(23, 23, 24)
    DropdownList.BackgroundTransparency = 0
    DropdownList.BorderSizePixel = 0

    -- Modify the DropdownList positioning to be more reasonable
    if isMobile then
        -- Position it below the dropdown but make it wider
        DropdownList.Position = UDim2.new(-0.2, 0, 1.1, 0)
        DropdownList.Size = UDim2.new(1.4, 0, 0, dropdownListHeight) -- 40% wider, moderate height
    else
        DropdownList.Position = UDim2.fromScale(0, 1.36)
        DropdownList.Size = UDim2.new(1, 0, 0, dropdownListHeight)
    end

    DropdownList.Visible = false
    DropdownList.ZIndex = 9999 -- Much higher Z-index to ensure it's above everything
    DropdownList.ClipsDescendants = true

    local UICornerList = Instance.new("UICorner", DropdownList)
    UICornerList.CornerRadius = UDim.new(0, 4)

    local UIStrokeList = Instance.new("UIStroke", DropdownList)
    UIStrokeList.ApplyStrokeMode = Enum.ApplyStrokeMode.Border
    UIStrokeList.Color = Color3.fromRGB(56, 56, 56)
    UIStrokeList.Transparency = 0.5

    -- Add background overlay to capture touches outside the dropdown
    local BackgroundOverlay = Instance.new("TextButton", DropdownHolder)
    BackgroundOverlay.Name = "BackgroundOverlay"
    BackgroundOverlay.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
    BackgroundOverlay.BackgroundTransparency = 0.7
    BackgroundOverlay.BorderSizePixel = 0
    BackgroundOverlay.Text = ""
    BackgroundOverlay.ZIndex = 9998 -- Just below the dropdown list
    BackgroundOverlay.Size = UDim2.fromScale(20, 20)
    BackgroundOverlay.Position = UDim2.fromScale(-10, -10)
    BackgroundOverlay.Visible = false
    BackgroundOverlay.AutoButtonColor = false

    local OptionHolder = Instance.new("ScrollingFrame", DropdownList)
    OptionHolder.Name = "Options"
    OptionHolder.BackgroundTransparency = 1
    OptionHolder.BorderSizePixel = 0
    OptionHolder.Size = UDim2.fromScale(1, 1)
    -- Adjust the scrollbar thickness to be more moderate
    OptionHolder.ScrollBarThickness = isMobile and 6 or 4
    OptionHolder.ZIndex = 10000 -- Above the dropdown list

    local UIListLayout = Instance.new("UIListLayout", OptionHolder)
    -- Adjust the padding to be more moderate
    UIListLayout.Padding = UDim.new(0, isMobile and 8 or 6) -- Moderate padding on mobile
    UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder

    local UIPadding = Instance.new("UIPadding", OptionHolder)
    -- Adjust the padding to be more moderate
    UIPadding.PaddingTop = UDim.new(0, isMobile and 6 or 2) -- Moderate padding on mobile
    UIPadding.PaddingBottom = UDim.new(0, isMobile and 10 or 6) -- Moderate padding on mobile
    UIPadding.PaddingLeft = UDim.new(0, isMobile and 10 or 0) -- Moderate padding on mobile
    UIPadding.PaddingRight = UDim.new(0, isMobile and 10 or 0) -- Moderate padding on mobile

    local chosen = Dropdown.Max and {} or nil
    local Count = 0

    -- Function to toggle dropdown visibility
    local function toggleDropdown(visible)
        DropdownList.Visible = visible
        BackgroundOverlay.Visible = visible
        
        if visible then
            DropdownFrame.ZIndex = 59
            TweenService:Create(
                DropdownName,
                TweenInfo.new(0.35, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                { TextColor3 = Color3.fromRGB(255, 255, 255) }
            ):Play()
            
            TweenService:Create(
                DropdownArrow,
                TweenInfo.new(0.35, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                { Rotation = 180 }
            ):Play()
        else
            DropdownFrame.ZIndex = 56
            TweenService:Create(
                DropdownName,
                TweenInfo.new(0.35, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                { TextColor3 = Color3.fromRGB(62, 62, 62) }
            ):Play()
            
            TweenService:Create(
                DropdownArrow,
                TweenInfo.new(0.35, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                { Rotation = 0 }
            ):Play()
        end
    end

    local function createoptions(tbl)
        for _, option in next, tbl do
            Dropdown.OptionInsts[option] = {}

            local Option = Instance.new("TextLabel", OptionHolder)
            Option.Name = option
            Option.FontFace = Font.new("rbxasset://fonts/families/Ubuntu.json")
            Option.Text = option
            Option.TextColor3 = Color3.fromRGB(120, 120, 120)
            Option.TextSize = 11
            Option.TextXAlignment = Enum.TextXAlignment.Left
            Option.BackgroundTransparency = 1
            Option.Size = UDim2.new(1, 0, 0, 22)
            Option.ZIndex = 10001

            local Button = Instance.new("TextButton", Option)
            Button.BackgroundTransparency = 1
            Button.Size = UDim2.fromScale(1, 1)
            Button.Text = ""
            Button.ZIndex = 10001

            local Padding = Instance.new("UIPadding", Option)
            Padding.PaddingLeft = UDim.new(0, 8)

            Dropdown.OptionInsts[option] = {
                button = Button,
                text = Option,
            }

            Count = Count + 1

            Library:Connection(Button.InputBegan, function(input)
                if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                    if Dropdown.Max then
                        if table.find(chosen, option) then
                            table.remove(chosen, table.find(chosen, option))
                            TweenService:Create(
                                Option,
                                TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                                { TextColor3 = Color3.fromRGB(120, 120, 120) }
                            ):Play()
                        else
                            if #chosen == Dropdown.Max then
                                table.remove(chosen, 1)
                            end
                            table.insert(chosen, option)
                            TweenService:Create(
                                Option,
                                TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                                { TextColor3 = Color3.fromRGB(255, 188, 254) }
                            ):Play()
                        end

                        local textchosen = {}
                        for _, opt in next, chosen do
                            table.insert(textchosen, opt)
                        end
                        DropdownText.Text = #chosen == 0 and "" or table.concat(textchosen, ",")
                    else
                        for opt, tbl in next, Dropdown.OptionInsts do
                            TweenService:Create(
                                tbl.text,
                                TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                                {
                                    TextColor3 = opt == option and Color3.fromRGB(255, 188, 254)
                                        or Color3.fromRGB(120, 120, 120),
                                }
                            ):Play()
                        end
                        chosen = option
                        DropdownText.Text = option
                        
                        -- Close dropdown after selection for non-multi-select dropdowns
                        toggleDropdown(false)
                    end

                    Library.Flags[Dropdown.Flag] = chosen
                    Dropdown.Callback(chosen)
                end
            end)

            if Dropdown.ScrollMax then
                OptionHolder.AutomaticCanvasSize = Enum.AutomaticSize.None
                if Count < Dropdown.ScrollMax then
                    OptionHolder.CanvasSize = UDim2.new(0, 0, 0, Count * 22)
                else
                    OptionHolder.CanvasSize = UDim2.new(0, 0, 0, Dropdown.ScrollMax * 22)
                end
            else
                OptionHolder.AutomaticCanvasSize = Enum.AutomaticSize.Y
            end
        end

        -- Toggle dropdown when clicking the dropdown frame
        Library:Connection(DropdownFrame.InputBegan, function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                toggleDropdown(not DropdownList.Visible)
            end
        end)

        -- Close dropdown when clicking the background overlay
        Library:Connection(BackgroundOverlay.InputBegan, function(input)
            if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
                toggleDropdown(false)
            end
        end)
    end
    
    createoptions(Dropdown.Options)

    function Dropdown.Set(self, option)
        if Dropdown.Max then
            table.clear(chosen)
            option = type(option) == "table" and option or {}

            for opt, tbl in next, Dropdown.OptionInsts do
                if not table.find(option, opt) then
                    TweenService
                        :Create(
                            tbl.text,
                            TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {
                                TextColor3 = Color3.fromRGB(120, 120, 120),
                            }
                        )
                        :Play()
                end
            end

            for _, opt in next, option do
                if table.find(Dropdown.Options, opt) and #chosen < Dropdown.Max then
                    table.insert(chosen, opt)
                    TweenService:Create(
                        Dropdown.OptionInsts[opt].text,
                        TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                        {
                            TextColor3 = Color3.fromRGB(255, 188, 254),
                        }
                    ):Play()
                end
            end

            local textchosen = {}
            for _, opt in next, chosen do
                table.insert(textchosen, opt)
            end
            DropdownText.Text = #chosen == 0 and "" or table.concat(textchosen, ", ")
        else
            if table.find(Dropdown.Options, option) then
                chosen = option
                DropdownText.Text = option
                for opt, tbl in next, Dropdown.OptionInsts do
                    TweenService
                        :Create(
                            tbl.text,
                            TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out),
                            {
                                TextColor3 = opt == option and Color3.fromRGB(255, 188, 254)
                                or Color3.fromRGB(120, 120, 120),
                            }
                        )
                        :Play()
                end
            end
        end

        Library.Flags[Dropdown.Flag] = chosen
        Dropdown.Callback(chosen)
    end

    function Dropdown.Refresh(self, tbl)
        for _, opt in next, Dropdown.OptionInsts do
            opt.text:Destroy()
        end
        table.clear(Dropdown.OptionInsts)
        Dropdown.Options = tbl
        createoptions(tbl)

        if Dropdown.Max then
            table.clear(chosen)
        else
            chosen = nil
        end

        DropdownText.Text = ""
        Library.Flags[Dropdown.Flag] = chosen
        Dropdown.Callback(chosen)
    end

    if Dropdown.Default then
        if Dropdown.Max then
            local defaultOptions = type(Dropdown.Default) == "table" and Dropdown.Default
                or { Dropdown.Default }
            Dropdown:Set(defaultOptions)
        else
            Dropdown:Set(Dropdown.Default)
        end
    end

    return Dropdown
end



		function Sections.Slider(self, Properties)
			local Slider = {
				Name = Properties.Name or "Slider",
				Min = Properties.Min or 0,
				Max = Properties.Max or 100,
				Default = Properties.Default or 50,
				Decimals = Properties.Decimals or 1,
				Flag = Properties.Flag or Library.NextFlag(),
				Callback = Properties.Callback or function() end,
				Section = self,
			}

			local RealSlider = Instance.new("TextButton", Slider.Section.Elements.SectionContent)
			RealSlider.Name = "RealSlider"
			RealSlider.Text = ""
			RealSlider.Active = false
			RealSlider.BackgroundTransparency = 1
			RealSlider.BorderSizePixel = 0
			RealSlider.Selectable = false
			RealSlider.Size = UDim2.new(1, 0, 0, 25)

			local UIPadding = Instance.new("UIPadding", RealSlider)
			UIPadding.PaddingBottom = UDim.new(0, 10)

			local SliderText = Instance.new("TextLabel", RealSlider)
			SliderText.Name = "TextLabel"
			SliderText.FontFace = Font.new("rbxassetid://12187365364")
			SliderText.Text = Slider.Name
			SliderText.TextColor3 = Color3.fromRGB(62, 62, 62)
			SliderText.TextSize = 12
			SliderText.TextWrapped = true
			SliderText.TextXAlignment = Enum.TextXAlignment.Left
			SliderText.AnchorPoint = Vector2.new(0, 0.5)
			SliderText.BackgroundTransparency = 1
			SliderText.Position = UDim2.new(0, 11, 0.5, -1)
			SliderText.Size = UDim2.new(1, -52, 0, 17)

			local SliderBar = Instance.new("Frame", SliderText)
			SliderBar.Name = "Frame"
			SliderBar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			SliderBar.BackgroundTransparency = 1
			SliderBar.Position = UDim2.new(0, -1, 1, 4)
			SliderBar.Size = UDim2.new(1, 24, 0, 5)

			local SliderCount = Instance.new("TextBox", SliderBar)
			SliderCount.Name = "SliderCount"
			SliderCount.FontFace = Font.new("rbxassetid://12187365364")
			SliderCount.RichText = true
			SliderCount.Text = tostring(Slider.Default)
			SliderCount.TextColor3 = Color3.fromRGB(100, 100, 100)
			SliderCount.TextSize = 12
			SliderCount.TextXAlignment = Enum.TextXAlignment.Right
			SliderCount.TextYAlignment = Enum.TextYAlignment.Top
			SliderCount.BackgroundTransparency = 1
			SliderCount.Position = UDim2.fromOffset(48, -17)
			SliderCount.Size = UDim2.new(1, -48, 1, 2)
			SliderCount.ClipsDescendants = false

			local UICorner = Instance.new("UICorner", SliderBar)
			UICorner.CornerRadius = UDim.new(0, 24)

			local SliderIndicator = Instance.new("Frame", SliderBar)
			SliderIndicator.Name = "RealSlider_indicator"
			SliderIndicator.BackgroundColor3 = Color3.fromRGB(158, 114, 158)
			SliderIndicator.BorderSizePixel = 0
			SliderIndicator.Size = UDim2.new(0, 0, 1, 0)

			local Indicator = Instance.new("Frame", SliderIndicator)
			Indicator.Name = "Frame"
			Indicator.BackgroundColor3 = Color3.fromRGB(255, 188, 254)
			Indicator.BorderSizePixel = 0
			Indicator.Position = UDim2.fromScale(1, -0.6)
			Indicator.Size = UDim2.fromOffset(11, 11)

			Instance.new("UICorner", Indicator).CornerRadius = UDim.new(0, 34)
			Instance.new("UICorner", SliderIndicator).CornerRadius = UDim.new(0, 24)

			local UIStroke = Instance.new("UIStroke", SliderBar)
			UIStroke.Color = Color3.fromRGB(30, 30, 30)

			local Sliding = false
			local _ = Slider.Default

			local function Set(value)
				value = math.clamp(Library:Round(value, Slider.Decimals), Slider.Min, Slider.Max)
				local sizeX = ((value - Slider.Min) / (Slider.Max - Slider.Min))

				TweenService:Create(SliderIndicator, TweenInfo.new(0.20, Enum.EasingStyle.Sine), {
					Size = UDim2.new(sizeX, 0, 1, 0),
				}):Play()

				SliderCount.Text = tostring(value)
				Library.Flags[Slider.Flag] = value
				Slider.Callback(value)
			end

			local function SlideInp(input)
				local sizeX = (input.Position.X - SliderBar.AbsolutePosition.X) / SliderBar.AbsoluteSize.X
				local value = ((Slider.Max - Slider.Min) * sizeX) + Slider.Min
				Set(value)
			end

			local function ConnectToInput(obj)
				obj.InputBegan:Connect(function(input)
					if
						input.UserInputType == Enum.UserInputType.MouseButton1
						or input.UserInputType == Enum.UserInputType.Touch
					then
						Sliding = true
						SlideInp(input)
						TweenService:Create(SliderText, TweenInfo.new(0.25, Enum.EasingStyle.Sine), {
							TextColor3 = Color3.fromRGB(255, 255, 255),
						}):Play()
					end
				end)

				obj.InputEnded:Connect(function(input)
					if
						input.UserInputType == Enum.UserInputType.MouseButton1
						or input.UserInputType == Enum.UserInputType.Touch
					then
						Sliding = false
						TweenService:Create(SliderText, TweenInfo.new(0.25, Enum.EasingStyle.Sine), {
							TextColor3 = Color3.fromRGB(62, 62, 62),
						}):Play()
					end
				end)
			end

			ConnectToInput(SliderBar)
			ConnectToInput(SliderIndicator)
			ConnectToInput(Indicator)

			game:GetService("UserInputService").InputChanged:Connect(function(input)
				if
					(
						input.UserInputType == Enum.UserInputType.MouseMovement
							or input.UserInputType == Enum.UserInputType.Touch
					) and Sliding
				then
					SlideInp(input)
				end
			end)

			SliderCount.Focused:Connect(function()
				TweenService:Create(SliderText, TweenInfo.new(0.20, Enum.EasingStyle.Sine), {
					TextColor3 = Color3.fromRGB(255, 255, 255),
				}):Play()
			end)

			SliderCount.FocusLost:Connect(function()
				local newValue = tonumber(SliderCount.Text)
				if newValue then
					Set(newValue)
				else
					SliderCount.Text = tostring(Library.Flags[Slider.Flag])
				end
				TweenService:Create(SliderText, TweenInfo.new(0.25, Enum.EasingStyle.Sine), {
					TextColor3 = Color3.fromRGB(62, 62, 62),
				}):Play()
			end)

			Set(Slider.Default)

			function Slider.Set(self, value)
				Set(value)
			end

			return Slider
		end

		function Sections.Button(self, Properties)
			local Button = {
				Window = self.Window,
				Page = self.Page,
				Section = self,
				Name = Properties.Name or "button",
				Callback = Properties.Callback or function() end,
			}

			local ButtonFrame = Instance.new("Frame")
			ButtonFrame.Name = "Button_frame"
			ButtonFrame.BackgroundColor3 = Color3.fromRGB(28, 29, 32)
			ButtonFrame.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ButtonFrame.BackgroundTransparency = 1
			ButtonFrame.BorderSizePixel = 0
			ButtonFrame.Size = UDim2.new(1, 0, 0, 30)

			local ButtonDetector = Instance.new("TextButton")
			ButtonDetector.Name = "Button_detector"
			ButtonDetector.AnchorPoint = Vector2.new(0.5, 0.5)
			ButtonDetector.FontFace = Font.new("rbxasset://fonts/families/SourceSansPro.json")
			ButtonDetector.Text = ""
			ButtonDetector.TextColor3 = Color3.fromRGB(0, 0, 0)
			ButtonDetector.TextSize = 14
			ButtonDetector.BackgroundColor3 = Color3.fromRGB(28, 29, 32)
			ButtonDetector.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ButtonDetector.BorderSizePixel = 0
			ButtonDetector.Size = UDim2.new(0, 191, 0, 26)
			ButtonDetector.Position = UDim2.new(0.5, 0, 0.5, 0)
			ButtonDetector.AutoButtonColor = false
			ButtonDetector.Parent = ButtonFrame

			local ButtonName = Instance.new("TextLabel")
			ButtonName.Name = "Button_name"
			ButtonName.AnchorPoint = Vector2.new(0.5, 0.5)
			ButtonName.FontFace = Font.new("rbxasset://fonts/families/Ubuntu.json")
			ButtonName.Text = Button.Name
			ButtonName.TextColor3 = Color3.fromRGB(120, 120, 120)
			ButtonName.TextSize = 13
			ButtonName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
			ButtonName.BackgroundTransparency = 1
			ButtonName.BorderColor3 = Color3.fromRGB(0, 0, 0)
			ButtonName.BorderSizePixel = 0
			ButtonName.Position = UDim2.new(0.5, 0, 0.5, 0)
			ButtonName.Size = UDim2.new(0, 189, 0, 26)
			ButtonName.Parent = ButtonFrame

			local UICorner = Instance.new("UICorner")
			UICorner.CornerRadius = UDim.new(0, 6)
			UICorner.Parent = ButtonDetector

			ButtonFrame.Parent = Button.Section.Elements.SectionContent

			ButtonDetector.InputBegan:Connect(function(input)
				if
					input.UserInputType == Enum.UserInputType.MouseButton1
					or input.UserInputType == Enum.UserInputType.Touch
				then
					Button.Callback()
					TweenService
						:Create(ButtonName, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
							TextColor3 = Color3.fromRGB(255, 255, 255),
						})
						:Play()
				end
			end)

			ButtonDetector.InputEnded:Connect(function(input)
				if
					input.UserInputType == Enum.UserInputType.MouseButton1
					or input.UserInputType == Enum.UserInputType.Touch
				then
					TweenService
						:Create(ButtonName, TweenInfo.new(0.25, Enum.EasingStyle.Quad, Enum.EasingDirection.Out), {
							TextColor3 = Color3.fromRGB(120, 120, 120),
						})
						:Play()
				end
			end)

			return Button
		end

		function Sections.Colorpicker(self, Properties)
			local Properties = Properties or {}
			local Colorpicker = {
				Section = self,
				Name = (Properties.Name or "Colorpicker"),
				State = (Properties.State or Color3.fromRGB(255, 0, 0)),
				Alpha = (Properties.Alpha or 1),
				Callback = (Properties.Callback or function() end),
				Flag = (Properties.Flag or Library.NextFlag()),
				Colorpickers = 0,
			}

			local colorpicker22 = Instance.new("TextButton", Colorpicker.Section.Elements.SectionContent)
			colorpicker22.Name = "Colorpicker22"
			colorpicker22.Text = ""
			colorpicker22.AutoButtonColor = false
			colorpicker22.BackgroundTransparency = 1
			colorpicker22.Size = UDim2.new(1, 0, 0, 25)

			local colorpickername = Instance.new("TextLabel", colorpicker22)
			colorpickername.Name = "Colorpickername"
			colorpickername.FontFace = Font.new("rbxassetid://12187365364")
			colorpickername.Text = Colorpicker.Name
			colorpickername.TextColor3 = Color3.fromRGB(62, 62, 62)
			colorpickername.TextSize = 12
			colorpickername.TextXAlignment = Enum.TextXAlignment.Left
			colorpickername.BackgroundTransparency = 1
			colorpickername.Position = UDim2.new(0, 11, 0.5, 0)
			colorpickername.Size = UDim2.new(1, -102, 0, 17)
			colorpickername.AnchorPoint = Vector2.new(0, 0.5)

			local colorpickerframe = Instance.new("Frame", colorpicker22)
			colorpickerframe.Name = "Colorpickerframe"
			colorpickerframe.AnchorPoint = Vector2.new(1, 0.5)
			colorpickerframe.BackgroundTransparency = 1
			colorpickerframe.BackgroundColor3 = Colorpicker.State
			colorpickerframe.Position = UDim2.new(0.996, -15, 0.5, 0)
			colorpickerframe.Size = UDim2.fromOffset(15, 15)

			local uICorner = Instance.new("UICorner", colorpickerframe)
			uICorner.CornerRadius = UDim.new(0, 5)

			Colorpicker.Colorpickers = Colorpicker.Colorpickers + 1
			local colorpickertypes = Library:NewPicker(
				Colorpicker.Name,
				Colorpicker.State,
				Colorpicker.Alpha,
				colorpickerframe,
				Colorpicker.Colorpickers,
				Colorpicker.Flag,
				function(color)
					Colorpicker.Callback(color)
				end
			)

			function Colorpicker.Set(self, color)
				colorpickertypes:Set(color)
			end

			return Colorpicker
		end

        function Sections.TextLabel(self, Properties)
    local Properties = Properties or {}
    local TextLabel = {
        Section = self,
        Text = Properties.Text or "",
        TextColor = Properties.TextColor or Color3.fromRGB(120, 120, 120), -- Default color matching UI
        TextSize = Properties.TextSize or 12,
        Flag = Properties.Flag or Library.NextFlag(),
    }

    -- Create the main frame
    local TextLabelFrame = Instance.new("Frame", TextLabel.Section.Elements.SectionContent)
    TextLabelFrame.Name = "TextLabel"
    TextLabelFrame.BackgroundTransparency = 1
    TextLabelFrame.Size = UDim2.new(1, 0, 0, 0)
    TextLabelFrame.AutomaticSize = Enum.AutomaticSize.Y

    -- Create the text label
    local TextLabelInstance = Instance.new("TextLabel", TextLabelFrame)
    TextLabelInstance.Name = "TextLabelInstance"
    TextLabelInstance.FontFace = Font.new("rbxassetid://12187365364")
    TextLabelInstance.Text = TextLabel.Text
    TextLabelInstance.TextColor3 = TextLabel.TextColor
    TextLabelInstance.TextSize = TextLabel.TextSize
    TextLabelInstance.TextXAlignment = Enum.TextXAlignment.Left
    TextLabelInstance.TextWrapped = true
    TextLabelInstance.BackgroundTransparency = 1
    TextLabelInstance.Position = UDim2.new(0, 11, 0, 0)
    TextLabelInstance.Size = UDim2.new(1, -22, 0, 0)
    TextLabelInstance.AutomaticSize = Enum.AutomaticSize.Y

    -- Methods for updating the TextLabel
    function TextLabel:Set(text)
        TextLabelInstance.Text = text
        Library.Flags[TextLabel.Flag] = text
    end

    function TextLabel:SetColor(color)
        TextLabelInstance.TextColor3 = color
    end

    Library.Flags[TextLabel.Flag] = TextLabel.Text
    
    return TextLabel
end

		return Library
	end
end
